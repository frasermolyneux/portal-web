{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "chart.js@4.4.3",
      "provider": "unpkg",
      "destination": "wwwroot/lib/chart.js",
      "files": [
        "dist/chart.umd.js"
      ]
    },
    {
      "library": "moment.js@2.30.1",
      "destination": "wwwroot/lib/moment",
      "files": [
        "moment.min.js"
      ]
    },
    {
      "library": "datatables.net-dt@1.13.8",
      "destination": "wwwroot/lib/datatables",
      "files": [
        "css/jquery.dataTables.min.css"
      ]
    },
    {
      "library": "datatables.net@1.13.8",
      "destination": "wwwroot/lib/datatables",
      "files": [
        "jquery.dataTables.min.js"
      ]
    },
    {
      "library": "datatables.net-responsive-dt@2.5.0",
      "destination": "wwwroot/lib/datatables-responsive",
      "files": [
        "responsive.dataTables.min.css"
      ]
    },
    {
      "library": "datatables.net-responsive@2.5.0",
      "destination": "wwwroot/lib/datatables-responsive",
      "files": [
        "dataTables.responsive.min.js"
      ]
    },
    {
      "library": "jquery@3.7.1",
      "provider": "unpkg",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.8",
      "provider": "unpkg",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "@fortawesome/fontawesome-free@6.7.2",
      "provider": "cdnjs",
      "destination": "wwwroot/lib/font-awesome",
      "files": [
        "css/all.min.css",
        "css/all.css",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-brands-400.ttf"
      ]
    },
    {
      "library": "pace-js@1.2.4",
      "provider": "unpkg",
      "destination": "wwwroot/lib/pace",
      "files": [
        "pace.js",
        "pace.min.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    }
  ]
}